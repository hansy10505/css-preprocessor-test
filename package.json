{
  "name": "css-preprocessor",
  "version": "1.0.0",
  "description": "基于原生JS+HTML5+CSS3实现的在线个人简历",
  "main": "index.js",
  "scripts": {
    "start": "npm run dev",
    "server": "http-server -p 9010 ./dist",
    "dev": "cross-env NODE_ENV=dev webpack-dev-server --config ./build/webpack.config.dev.js --progress --colors --watch",
    "build": "cross-env NODE_ENV=production webpack --config ./build/webpack.config.prod.js --progress --colors"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mgss/css-preprocessor.git"
  },
  "author": "mgss",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mgss/css-preprocessor/issues"
  },
  "homepage": "https://github.com/mgss/css-preprocessor#readme",
  "devDependencies": {
    "autoprefixer": "^7.2.2",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "clean-webpack-plugin": "^0.1.17",
    "cross-env": "^7.0.3",
    "css-loader": "^0.28.7",
    "eslint": "^4.13.1",
    "eslint-loader": "^1.9.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.5",
    "html-loader": "^0.5.1",
    "html-webpack-plugin": "^2.30.1",
    "less": "^2.7.3",
    "less-loader": "^4.0.5",
    "node-sass": "yarn:dart-sass",
    "open-browser-webpack-plugin": "0.0.5",
    "postcss-import": "^11.0.0",
    "postcss-loader": "^2.0.9",
    "sass": "^1.53.0",
    "sass-loader": "7.3.1",
    "style-loader": "^0.19.0",
    "uglifyjs-webpack-plugin": "^1.1.3",
    "url-loader": "^0.6.2",
    "webpack": "^3.10.0",
    "webpack-dev-server": "^2.9.7",
    "webpack-merge": "^4.1.1"
  },
  "dependencies": {
    "expose-loader": "^0.7.4",
    "jquery": "^1.12.4"
  }
}
